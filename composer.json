{
    "name": "spatie/sheets",
    "description": "Store & retrieve your static content in plain text files",
    "keywords": [
        "spatie",
        "laravel",
        "sheets"
    ],
    "homepage": "https://github.com/spatie/sheets",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/filesystem": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "league/commonmark": "^1.0",
        "spatie/yaml-front-matter": "^2.0",
        "symfony/yaml": "^4.0|^5.0"
    },
    "require-dev": {
        "league/flysystem": "^1.0.8",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Sheets\\SheetsServiceProvider"
            ],
            "aliases": {
                "Sheets": "Spatie\\Sheets\\Facades\\Sheets"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Sheets\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Sheets\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
